<odoo>
    <record id="field_service_foc_fsm_person_form" model="ir.ui.view">
        <field name="name">field_service_foc_fsm_person_form</field>
        <field name="model">fsm.person</field>
        <field name="inherit_id" ref="fieldservice.fsm_person_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_ids']" position="after">
                <field name="user_id" />
            </xpath>
        </field>
    </record>
</odoo>
