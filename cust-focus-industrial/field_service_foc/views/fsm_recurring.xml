<odoo>
    <record id="fsm_recurring_form_view" model="ir.ui.view">
        <field name="name">fsm.recurring.form.inherit.focus</field>
        <field name="model">fsm.recurring</field>
        <field name="inherit_id" ref="fieldservice_recurring.fsm_recurring_form_view" />
        <field name="arch" type="xml">
            <field name="fsm_order_template_id" position="after">
                <field name="fsm_order_template_even_position_id" groups="fieldservice.group_fsm_template"/>
                <field name="fsm_order_template_odd_position_id" groups="fieldservice.group_fsm_template"/>
            </field>
        </field>
    </record>
</odoo>
